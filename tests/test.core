program
int X, Y;
int Z;
begin
read X;
if (X < 25) then
Y = 25;
write X;
else
X = X + 10;
end;
while !(X == 20) loop
X = X + Y;
Y = 10;
read Y;
end;
Y = X + 5 + 03 - Y + (X * (X - 1000) );
if [(5 <= (X + Y)) and ![ (X == Y) or (Y >= X)]] then
if (X > 25) then
while (X != 20) loop
read X;
end;
end;
end;
end